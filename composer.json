{
  "name": "ares-bagatin/woo-commerce-sdk",
  "description": "SDK for the WooCommerce API",
  "type": "library",
  "authors": [
    {
      "name": "Ares",
      "email": "ares.w1996@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4.1",
    "nesbot/carbon": "^2.57.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.13",
    "symfony/var-dumper": "^5.4",
    "fakerphp/faker": "^1.19"
  },
  "autoload": {
    "psr-4": {
      "WooCommerceSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WooCommerceSDK\\Tests\\": "tests/"
    }
  }
}
